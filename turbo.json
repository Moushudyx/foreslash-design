{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "serve": {
      "dependsOn": ["@fore-ui/utils#build", "fore-ui#build"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.js", "src/**/*.ts", "test/**/*.tsx", "test/**/*.jsx"]
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"],
      "outputs": []
    }
  }
}
